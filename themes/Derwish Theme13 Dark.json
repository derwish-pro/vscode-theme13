{
    "name": "Derwish Theme13 Dark",
    "author": "Derwish",
    "type": "dark",
    "tokenColors": [
        {
            "settings": {
                "background": "#2D3033",
                "foreground": "#EDE6E4"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#56606B"
            }
        },
        {
            "name": "Python Comments",
            "scope": [
                "source.python comment",
                "punctuation.definition.comment.python"
            ],
            "settings": {
                "foreground": "#898e99"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.meta.jsx",
                "variable.other.readwrite.js"
            ],
            "settings": {
                "foreground": "#EDE6E4"
            }
        },
        {
            "name": "Variable Properties",
            "scope": [
                "variable.other.property.static",
                "meta.property.object"
            ],
            "settings": {
                "foreground": "#EDE6E4"
            }
        },
        {
